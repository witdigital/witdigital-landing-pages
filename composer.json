{
  "name": "witlandingpages/witdigital-landing-pages",
  "description": "Provides pages templates and building blocks for lansing pages.",
  "type": "wordpress-plugin",
  "require": {
    "advanced-custom-fields/advanced-custom-fields-pro": "^5.9",
    "yahnis-elsts/plugin-update-checker": "^4.11",
    "scssphp/scssphp": "^1.10.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://pivvenit.github.io/acf-composer-bridge/composer/v3/library/"
    }
  ],
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "@WitDigital",
      "email": "development@witdigital.com"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "lint": [
      "phpcs --ignore=index.php,vendor,resources,storage,dist --extensions=php --standard=PSR12 ."
    ]
  }
}
